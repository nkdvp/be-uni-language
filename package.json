{
  "name": "nodejs-express-template",
  "version": "1.0.0",
  "description": "nodejs-express-template",
  "type": "module",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "exports": "./dist/index.js",
  "scripts": {
    "start:dev": "node --es-module-specifier-resolution=node -r dotenv/config ./dist/index.js",
    "start": "node --es-module-specifier-resolution=node ./dist/index.js",
    "dev": "nodemon --watch './**/*.ts' --exec 'node --experimental-specifier-resolution=node --loader ts-node/esm' -r dotenv/config src/index.ts",
    "dev-watch": "nodemon --watch './**/*.ts' --inspect=0.0.0.0:9227 --exec 'node --experimental-specifier-resolution=node --loader ts-node/esm' -r dotenv/config src/index.ts",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./",
    "lint-fix": "eslint --fix --ext .js,.jsx,.ts,.tsx ./",
    "build": "tsc",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "maitienduy@gmail.com",
  "license": "ISC",
  "dependencies": {
    "@types/knex": "^0.16.1",
    "amqplib": "^0.10.3",
    "aws-sdk": "^2.1556.0",
    "axios": "^1.6.0",
    "chai": "^4.3.10",
    "cors": "^2.8.5",
    "elastic-apm-node": "^3.50.0",
    "express": "^4.18.2",
    "express-fileupload": "^1.4.3",
    "express-rate-limit": "^6.11.2",
    "express-slow-down": "^1.6.0",
    "fastest-validator": "^1.17.0",
    "helmet": "^7.0.0",
    "http-proxy-middleware": "^2.0.6",
    "ioredis": "^5.3.2",
    "kafkajs": "^2.2.4",
    "knex": "^2.5.1",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "mongodb": "^5.9.1",
    "mongoose": "^7.6.4",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.6.3",
    "npm": "^10.2.3",
    "rate-limit-redis": "^2.1.0",
    "redis": "^4.6.10",
    "triple-beam": "^1.4.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.3",
    "@types/aws-sdk": "^2.7.0",
    "@types/chai": "^4.3.9",
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.20",
    "@types/express-fileupload": "^1.4.4",
    "@types/express-slow-down": "^1.3.4",
    "@types/lodash": "^4.14.200",
    "@types/md5": "^2.3.4",
    "@types/mocha": "^10.0.3",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.8.10",
    "@types/rate-limit-redis": "^1.7.4",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-import": "^2.29.0",
    "husky": "^8.0.3",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}
